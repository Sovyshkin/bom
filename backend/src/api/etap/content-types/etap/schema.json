{
  "kind": "collectionType",
  "collectionName": "etaps",
  "info": {
    "singularName": "etap",
    "pluralName": "etaps",
    "displayName": "Этап"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "status_work": {
      "type": "string"
    },
    "who": {
      "type": "string"
    },
    "start": {
      "type": "datetime"
    },
    "finish": {
      "type": "datetime"
    },
    "elementId": {
      "type": "string"
    },
    "startedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "finishedBy": {
      "type": "relation",
      "relation": "manyToOne", 
      "target": "plugin::users-permissions.user"
    },
    "startedAt": {
      "type": "datetime"
    },
    "finishedAt": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": ["Не начат", "В работе", "Готов"],
      "default": "Не начат"
    },
    "order": {
      "type": "integer"
    }
  }
}
